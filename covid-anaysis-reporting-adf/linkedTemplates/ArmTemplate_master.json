{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-anaysis-reporting-adf"
		},
		"ls_adls_covid_reporting_dl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covid_reporting_dl'"
		},
		"ls_azblob_covid_reporting_sa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_azblob_covid_reporting_sa'"
		},
		"ls_adls_covid_reporting_dl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://coviddatareporting11dl.dfs.core.windows.net/"
		},
		"ls_ecdc_data_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"tr_copy_source_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/e7957b47-1a7c-4db5-81fb-0c5acaa6fff0/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/coviddatareporting11sa"
		},
		"tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
			"type": "string",
			"defaultValue": "coolPineapple/covid19-reporting-adf/main/ECDC_Source_Datatsets/hospital_admissions.csv"
		},
		"tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-anaysis-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_covid_reporting_dl_accountKey": {
						"value": "[parameters('ls_adls_covid_reporting_dl_accountKey')]"
					},
					"ls_azblob_covid_reporting_sa_connectionString": {
						"value": "[parameters('ls_azblob_covid_reporting_sa_connectionString')]"
					},
					"ls_adls_covid_reporting_dl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covid_reporting_dl_properties_typeProperties_url')]"
					},
					"ls_ecdc_data_http_properties_typeProperties_url": {
						"value": "[parameters('ls_ecdc_data_http_properties_typeProperties_url')]"
					},
					"tr_copy_source_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_copy_source_data_properties_typeProperties_scope')]"
					},
					"tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL": {
						"value": "[parameters('tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sourceRelativeURL')]"
					},
					"tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_hospitalised_data_csv_properties_pl_ingest_ecdc_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}